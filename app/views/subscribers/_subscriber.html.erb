<article id="<%= dom_id subscriber %>" class="py-6 prose dark:prose-invert">
    <p class="mb-0 font-semibold">
      Email
    </p>
    <p class="my-0">
      <%= subscriber.email %>
    </p>
  <time class="text-slate-600 dark:text-slate-400 text-xs mt-2" datetime="<%= subscriber.created_at.to_formatted_s(:long) %>">Created <%= time_ago_in_words(subscriber.created_at) + " ago" %></time>
</article>
